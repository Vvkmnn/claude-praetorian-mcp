{
  "name": "claude-praetorian-mcp",
  "version": "1.1.1",
  "description": "Aggressive context compaction MCP server for Claude Code - save and retrieve structured snapshots with 90%+ token savings",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "claude-praetorian-mcp": "./dist/index.js"
  },
  "files": [
    "dist/",
    "skill/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && chmod +x dist/index.js",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "typecheck": "tsc --noEmit",
    "test": "npm run typecheck && npm run lint",
    "prepublishOnly": "npm run build && npm run lint && npm run format:check",
    "prepare": "husky || true"
  },
  "keywords": [
    "claude",
    "claude-code",
    "mcp",
    "mcp-server",
    "context",
    "compaction",
    "token-optimization",
    "ai",
    "terminal",
    "cli",
    "npx",
    "zero-install",
    "anthropic",
    "llm",
    "developer-tools",
    "productivity",
    "memory",
    "session"
  ],
  "author": "Vvkmnn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vvkmnn/claude-praetorian-mcp.git"
  },
  "homepage": "https://github.com/Vvkmnn/claude-praetorian-mcp#readme",
  "bugs": {
    "url": "https://github.com/Vvkmnn/claude-praetorian-mcp/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/Vvkmnn"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@toon-format/toon": "^2.1.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/node": "^22.0.0",
    "eslint": "^9.18.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.4.2",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.18.0"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
